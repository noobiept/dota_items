{
    "env": {
        "browser": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "semi": ["error", "always"],
        "no-unused-vars": ["error", { "args": "none" }],
        "consistent-return": ["error"],
        "curly": ["error"],
        "eqeqeq": ["error"],
        "strict": ["error", "global"]
    }
}
